block vars
  - var nestLevel = ''
  - var pgTitle = 'Blue Button Toolkit | Support Consumers in Getting & Using Their Health Records'
!!! 5
html
  block head
    head
      title= pgTitle
      meta(name='viewport', content='width=device-width, initial-scale=1')
      meta(content='text/html; charset=UTF-8', http-equiv='Content-Type')
      meta(http-equiv='X-UA-Compatible', content='IE=edge')
      link(rel='shortcut icon', href='#{nestLevel}favicon.ico')

      script(src='//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js')
      script.
        window.jQuery || document.write('<script src="#{nestLevel}js/jquery-1.10.2.min.js">\x3C/script>')
      block mainstyle
        link(rel='stylesheet', href='#{nestLevel}css/bbtoolkit-combined.css')
      script.
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        //- ga('create', 'UA-23987631-2', 'auto');
        //- ga('send', 'pageview');
        $(document).ready(function() {
      script.
          $("a.goal-title, a.box-link-rapper, a.collapsed").each(function() {
              var href = $(this).attr("href");
              var target = $(this).attr("target");
              var text = $(this).text();
              $(this).click(function(event) { // when someone clicks these links
                  event.preventDefault(); // don't open the link yet
                  _gaq.push(["_trackEvent", "Links", "Clicked", text, , false]); // create a custom event
                  setTimeout(function() { // now wait 300 milliseconds...
                      window.open(href,(!target?"_self":target)); // ...and open the link as usual
                  },300);
              });
          });
       
  body
    block content

    block footer
      .section.footer
        .container
          ul.footer-links.list-inline.inline
            li.hidden-xs.force-inline: img.little-logo(src='#{nestLevel}img/bb-logo-depth-small-greybg.jpg')
            li: a(href='mailto:bluebutton@hhs.gov?subject=Feedback%20on%20Blue%20Button%20Toolkit', target='_blank') Send feedback
          p.fs-xxsmall The ‘Blue Button’ logo is a registered Service Mark of the U.S. Department of Health and Human Services.
    block mainscript
      script(src='#{nestLevel}js/bbtoolkit-combined.min.js')
